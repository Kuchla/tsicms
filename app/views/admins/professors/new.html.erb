<h1>CRIAR</h1>
<%= simple_form_for @professor, url: admins_professors_path, method: :post do |f| %>
  <%= f.input :name, required: true %>
  <%= f.input :lattes, required: true %>
  <%= f.input :occupation_area, required: true %>
  <%= f.input :email, required: true %>
  <%= f.button :submit %>
<% end %>