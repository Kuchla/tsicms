<div class="col-sm-6 col-xl-3">
  <div class="card">
    <div class="card-body d-flex flex-column">
      <h4><%= trainee.title %></h4>
      <div class="d-flex align-items-center pt-5 mt-auto">
        <div>
          <%= link_to trainee.title, trainees_path(trainee) %>
          <small class="d-block text-muted"><%= l(trainee.created_at, format: :long) %></small>
        </div>
      </div>
    </div>
  </div>
</div>
